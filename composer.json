{
  "name": "cloudtay/p-ripple-drive",
  "version": "v1.2.4",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Psc\\Drive\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "cclilshy",
      "email": "jingnigg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-sockets": "*",
    "ext-pdo": "*",
    "cclilshy/p-ripple-core": "^0.5.4",
    "amphp/mysql": "^3.0",
    "workerman/workerman": "^4.1"
  },
  "require-dev": {
    "illuminate/support": "*",
    "symfony/console": "*",
    "illuminate/http": "*",
    "illuminate/console": "*",
    "illuminate/container": "*",
    "topthink/framework": "*",
    "friendsofphp/php-cs-fixer": "*",
    "laravel/framework": "*",
    "workerman/webman-framework": "*",
    "phpunit/phpunit": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Psc\\Drive\\Laravel\\Provider"
      ]
    },
    "think": {
      "services": [
        "Psc\\Drive\\ThinkPHP\\Service"
      ]
    }
  }
}
